<%-include("include/head.ejs") %>
<form action="" class="container" method="post">
    <div class="form-floating mb-3 my-2">
        <input
            type="text"
            class="form-control"
            name="username"
            id="formId1"
            placeholder=""
        />
        <label for="formId1">User Name</label>
    </div>
    <div class="form-floating mb-3">
        <input
            type="text"
            class="form-control"
            name="title"
            id="formId1"
            placeholder=""
        />
        <label for="formId1">Title of Post</label>
    </div>
    <div class="form-floating mb-3">
        <input
            type="text"
            class="form-control"
            name="description"
            id="formId1"
            placeholder=""
        />
        <label for="formId1">Description</label>
    </div>
    <img
    src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQhLbotJ9BK2XMQJ-lxtHlAGM48Ven9BG-IGw&s"
    class="img-fluid rounded-top"
    alt=""
/>
    <div class="mb-3">
        <label for="" class="form-label my-2">Choose file</label>
        <input
            type="file"
            class="form-control"
            name=""
            id=""
            placeholder=""
            aria-describedby="fileHelpId"
        />
       
        
        <div id="fileHelpId" class="form-text">JPG/JPEG Only*</div>
    </div>
    <button
        type="submit"
        class="btn btn-primary"
    >
        Update
    </button>
    
</form>


<%-include("include/foot.ejs") %>